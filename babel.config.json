{                           
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "import",
      [{"libraryName": "antd", "style": true}]
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "babel-plugin-transform-require-ignore",
          {
            "extensions": [ ".less" ]
          }
        ]
      ]
    }
  }
}